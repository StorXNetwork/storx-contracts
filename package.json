{
  "name": "StorX",
  "version": "1.0.0",
  "description": "",
  "main": "truffle-config.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test-proxy": "truffle test ./test/testProxy/*.js",
    "test-token": "truffle test ./test/testToken/*.js",
    "test-all": "truffle test ",
    "solhint": "./node_modules/.bin/solhint -f table contracts/**/*.sol",
    "prettier:solidity": "./node_modules/.bin/prettier --write contracts/**/*.sol",
    "watch": "onchange  -d 1000 './contracts/*/*.sol' --  npm run prettier:solidity"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "onchange": "^7.1.0",
    "prettier": "^2.2.1",
    "prettier-plugin-solidity": "^1.0.0-beta.9",
    "solhint-plugin-prettier": "0.0.5",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "@openzeppelin/test-environment": "^0.1.4",
    "@openzeppelin/test-helpers": "^0.5.6",
    "bignumber.js": "^9.0.1",
    "chai": "^4.2.0",
    "chai-bignumber": "^3.0.0",
    "lodash": "^4.17.20",
    "mocha": "^8.1.3",
    "truffle": "^5.1.43",
    "web3": "^1.3.5"
  }
}